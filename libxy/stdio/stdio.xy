import posix~[Clib{headers=@{"unistd.h"}}] in c;
import libxy.string;
import libxy.error;
import libxy.fs;

struct StdIo {
    stdin  := File{fd=0};
    stdout := File{fd=1};
    stderr := File{fd=2};
}

;; Prints `msg` to standard output
*func print(msg: String, file := [Global~StdIo].stdout) -> void {
    file'write(msg);
}
